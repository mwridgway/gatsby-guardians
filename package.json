{
  "name": "gatsby-guardians",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:watch:assets\" \"vite\"",
    "build": "npm run process:assets && tsc && vite build",
    "preview": "vite preview",
    "watch:assets": "node tools/watch-assets.js",
    "process:assets": "node tools/process-assets.js",
    "deploy": "gh-pages -d dist -r https://${DEPLOY_TOKEN}@github.com/mwridgway/gatsby-guardians.git"
  },
  "dependencies": {
    "howler": "^2.2.4",
    "phaser": "^3.80.1",
    "phaser3-rex-plugins": "^1.60.6"
  },
  "devDependencies": {
    "@types/howler": "^2.2.11",
    "@types/node": "^20.10.5",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "glob": "^10.3.10",
    "pngjs": "^7.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-build": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
